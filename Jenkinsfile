pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Build stage!'
            }
        }
        stage('Test') {
            steps {
                echo 'Test stage!'
            }
        }
        stage('Package') {
            steps {
                echo 'Package stage!'
            }
        }
        stage('Publish') {
            steps {
                echo 'Publish stage!'
            }
        }
        stage('Deploy to k8s') {
            steps {
                echo 'Deploy to k8s!'
            }
        }

    }
}